{
	"name": "tryresult",
	"description": "A typescript library to get rid of try catches.",
	"keywords": [
		"typescript",
		"error-handling",
		"try-catch",
		"errors"
	],
	"version": "2.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/ahme-dev/tryresult.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"*.js",
		"*.mjs",
		"*.d.ts"
	],
	"main": "./index.js",
	"module": "./index.mjs",
	"exports": {
		"require": "./index.js",
		"import": "./index.mjs"
	},
	"types": "./index.d.ts",
	"scripts": {
		"build-fast": "tsup src --format cjs,esm",
		"build": "pnpm run build-fast --dts",
		"copy-files": "cp dist/*.js dist/*.mjs dist/*.d.ts .",
		"clean": "rm -f *.js *.mjs *.d.ts",
		"test": "vitest run",
		"size": "size-limit",
		"prepublishOnly": "pnpm run clean && pnpm run build && pnpm run copy-files"
	},
	"size-limit": [
		{
			"path": "src/index.ts"
		}
	],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.0.5",
		"@size-limit/preset-small-lib": "^11.2.0",
		"@types/node": "^20.2.1",
		"size-limit": "^11.2.0",
		"tsup": "6.6.3",
		"typescript": "4.9.5",
		"vitest": "0.28.5"
	},
	"type": "module"
}